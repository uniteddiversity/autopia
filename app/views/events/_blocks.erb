<div class="row" id="events">
  <% events.each { |event| %>
  <div data-pagelet-url="/events/<%=event.id%>/block" class="col-12 mb-3 <%=block_class if defined?(block_class)%>" id="<%=event.id%>">
    <hr />
    <%= partial :'events/block', :locals => {:event => event} %>        
  </div>  
  <% } %>  
</div>