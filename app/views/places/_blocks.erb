<div class="row" id="places">
  <% places.each { |place| %>  
    <div data-pagelet-url="/places/<%=place.id%>/block" class="col-6 col-sm-3 mb-3 <%=block_class if defined?(block_class)%>" id="<%=place.id%>">
      <%= partial :'places/block', :locals => {:place => place} %>
    </div>
  <% } %>
</div>